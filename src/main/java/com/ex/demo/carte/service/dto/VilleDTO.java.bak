package com.ex.demo.carte.service.dto;

import com.ex.demo.carte.VilleType;
import com.ex.demo.carte.domaine.Ressource;


public class VilleDTO  {


    private Long id;

    private String nom;

    private VilleType villeType;


    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getNom() {
        return nom;
    }

    public void setNom(String nom) {
        this.nom = nom;
    }

    public VilleType getVilleType() {
        return villeType;
    }

    public void setVilleType(VilleType villeType) {
        this.villeType = villeType;
    }

}
